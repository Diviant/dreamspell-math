{
  "name": "dreamspell-math",
  "version": "0.2.0",
  "description": "dreamspell-math",
  "license": "MIT",
  "homepage": "https://github.com/oshimish/dreamspell-math#readme",
  "author": {
    "name": "Raman Oshimish",
    "email": "pustotahere@gmail.com"
  },
  "keywords": [
    "dreamspell",
    "maya",
    "time",
    "13-20",
    "zolkin"
  ],
  "bugs": {
    "url": "https://github.com/oshimish/dreamspell-math/issues"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/oshimish/dreamspell-math.git"
  },
  "exports": {
    ".": {
      "import": {
        "types": "./lib/esm/types/index.d.ts",
        "default": "./lib/esm/index.mjs"
      },
      "require": {
        "types": "./lib/cjs/types/index.d.ts",
        "default": "./lib/cjs/index.js"
      }
    }
  },
  "types": "./lib/cjs/types/index.d.ts",
  "main": "./lib/cjs/index.js",
  "files": [
    "lib/**/*"
  ],
  "scripts": {
    "clean": "rimraf lib && rimraf coverage",
    "lint": "tslint --force --format verbose \"src/**/*.ts\"",
    "prepare_disabled": "npm run build",
    "prebuild": "npm run clean && npm run format && npm run lint && echo Using TypeScript && tsc --version",
    "build": "npm run clean && npm run build:esm && npm run build:cjs",
    "build:esm": "tsc -p ./configs/tsconfig.esm.json && move-cli lib/esm/index.js lib/esm/index.mjs",
    "build:cjs": "tsc -p ./configs/tsconfig.cjs.json",
    "test": "jest",
    "semantic-release": "semantic-release",
    "coverage": "jest --coverage",
    "watch": "tsc --watch",
    "watch:test": "jest --watch"
  },
  "dependencies": {
    "moment": "^2.29.4"
  },
  "devDependencies": {
    "@types/jest": "^29.5.1",
    "@types/node": "^20.2.1",
    "fs-extra": "^11.1.1",
    "jest": "^29.5.0",
    "move-cli": "^2.0.0",
    "prettier": "^2.8.8",
    "rimraf": "^5.0.1",
    "semantic-release": "^19.0.3",
    "ts-jest": "^29.1.0",
    "ts-node": "^10.9.1",
    "tslint": "^6.1.3",
    "tslint-config-prettier": "^1.18.0",
    "typescript": "^4.7.4"
  },
  "engines": {
    "node": ">=6.0.0"
  }
}
